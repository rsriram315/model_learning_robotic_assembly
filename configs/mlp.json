{
    "name": "mlp",
    "train": true,
    "evaluate": false,
    "visualize": false,
    "rollout": false,
    "mpc": true,
    "dataset": {
        "name": "DemoDataset",
        "seed": 42,
        "root": "data",
        "fnames": [],
        "sample_freq": 1000,
        "sl_factor": 50,
        "contact_only": true,
        "state_attrs": {
            "name": "PandaStatePublisherarm_states",
            "attrs": ["tcp_pose_base", "tcp_wrench_ee"]
        },
        "action_attrs": {
            "name": "PandaCartesianImpedanceControllercontrollerReference",
            "attrs": ["pose", "wrench"]
        },
        "preprocess": {
            "normalize": true,
            "interp": {
                "pos": "cubic_spline",
                "rot": "slerp",
                "force": "cubic_spline"
            }
        },
        "stats": {
            "stat_1": null,
            "stat_2": null,
            "stat_3": null,
            "stat_4": null
        }
    },
    "dataloader": {
        "name": "DemoDataLoader",
        "batch_size": 1024,
        "shuffle": true,
        "validation_split": 0.2,
        "num_workers": 4
    },
    "model": {
        "name": "MLP",
        "n_gpu": 0,
        "input_dims": 30,
        "output_dims": 12
    },
    "trainer": {
        "name": "mlp",
        "num_epochs": 500,
        "ckpts_dir": "saved/ckpts/",
        "save_period": 5,
        "early_stop": true,
        "patience": 5,
        "log_file": "saved/train.log",
        "tb_dir": "saved/tb_log"
    },
    "optimizer": {
        "name": "Adam",
        "lr": 0.001,
        "weight_decay": 0,
        "amsgrad": true
    },
    "eval": {
        "ckpt_dir": "saved/ckpts/",
        "ckpt_pth": null,
        "log_file": "saved/evaluation.log"
    },
    "visualization": {
        "loss": true,
        "axis": true,
        "trajectory": true,
        "contact": false
    }
}
